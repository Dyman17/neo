{
  "name": "archaeoscan-monorepo",
  "version": "1.0.0",
  "description": "Archaeoscan Monorepo - Frontend + Backend",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python -m uvicorn src.main:app --host 0.0.0.0 --port 7860 --reload",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "build:frontend": "cd frontend && npm run build",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && pip install -r requirements.txt",
    "install:all": "npm run install:frontend && npm run install:backend"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["archaeology", "monorepo", "websocket", "fastapi", "react"],
  "author": "Dyman17",
  "license": "MIT"
}
